{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2021",
    "sourceMap": true,
    "incremental": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {}
  },
  "exclude": ["dist", "node_modules"]
}
